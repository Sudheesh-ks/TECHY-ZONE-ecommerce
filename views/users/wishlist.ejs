<!-- Wishlist Table -->
<div class="container wishlist-table">
    <div class="row">
        <div class="col-12">
            <h5><a href="#">Home</a> > Wishlist</h5>
            <table class="table table-bordered mt-3">
                <thead class="thead-light">
                    <tr>
                        <th>Product</th>
                        <th>Name</th>
                        <th>Price</th>
                        <th>Stock Status</th>
                        <th>Add to Cart</th>
                        <th>Remove from Cart</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td><img src="images/Wishlist-Monitor 1.png" alt="Monitor" class="img-fluid"></td>
                        <td>Monitor</td>
                        <td>₹ 64,671.00</td>
                        <td><span class="text-success fw-bold">In Stock</span></td>
                        <td><button class="btn btn-custom">Add to Cart</button></td>
                        <td><button class="btn btn-danger">Remove</button></td>
                    </tr>
                    <tr>
                        <td><img src="images/Wishlist-Table.png" alt="SmartDesk" class="img-fluid"></td>
                        <td>SmartDesk</td>
                        <td>₹ 8,500</td>
                        <td><span class="text-success fw-bold">In Stock</span></td>
                        <td><button class="btn btn-custom">Add to Cart</button></td>
                        <td><button class="btn btn-danger">Remove</button></td>
                    </tr>
                </tbody>
            </table>
        </div>
