    <!-- Main Content -->
    <div class="container mt-5">
        <div class="row">
            <!-- Shipping Method Section -->
            <div class="col-md-8">
                <h5>Shipping Method</h5>
                <div class="card p-3">
                    <div class="form-check">
                        <input class="form-check-input" type="radio" name="shippingMethod" id="freeShipping" checked>
                        <label class="form-check-label" for="freeShipping">
                            Free Regular Shipment - 01 Feb, 2023
                        </label>
                    </div>
                    <div class="form-check mt-2">
                        <input class="form-check-input" type="radio" name="shippingMethod" id="priorityShipping">
                        <label class="form-check-label" for="priorityShipping">
                            $8.50 Priority Shipping - 28 Jan, 2023
                        </label>
                    </div>
                    <div class="form-check mt-2">
                        <input class="form-check-input" type="radio" name="shippingMethod" id="scheduleShipping">
                        <label class="form-check-label" for="scheduleShipping">
                            Schedule - Choose a date that works for you.
                        </label>
                        <input type="date" class="form-control mt-2" placeholder="Select Date">
                    </div>
                </div>
            </div>

            <!-- Order Summary Section -->
            <div class="col-md-4">
                <div class="card p-3">
                    <h5>Order Summary</h5>
                    <p>Price: <span class="float-right">$319.98</span></p>
                    <p>Discount: <span class="float-right">$31.9</span></p>
                    <p>Shipping: <span class="float-right">Free</span></p>
                    <p>Coupon Applied: <span class="float-right">$0.00</span></p>
                    <hr>
                    <p>Total: <span class="float-right">$288.08</span></p>
                    <p>Estimated Delivery by <span class="float-right">01 Feb, 2023</span></p>
                    <input type="text" class="form-control" placeholder="Coupon Code">
                    <button class="btn btn-dark btn-block mt-2">Continue to Payment</button>
                </div>
            </div>
        </div>
